class Solution {
public:
    int maxProfit(vector<int>& nums) {
        int bp=nums[0];
        int p=0;
        for(int i=1;i<nums.size();i++)
        {
            if(bp>nums[i])
            {
                bp=nums[i];
            }
            p=max(p,nums[i]-bp);
        }
        return p;
        
    }
};
